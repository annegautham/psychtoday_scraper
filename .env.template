# Environment Variables for Therapist Outreach Script
# Copy this file to .env and fill in your actual values

# Email Configuration
EMAIL_ADDRESS=outreach@koamigo.com
EMAIL_PASSWORD=your_actual_password_here
SMTP_SERVER=mail.privateemail.com
SMTP_PORT=465

# Hunter.io API (optional - for better email discovery)
HUNTER_IO_API_KEY=your_hunter_io_api_key_here

# Rate Limiting
MAX_EMAILS_PER_DAY=50
DELAY_BETWEEN_EMAILS_MIN=60
DELAY_BETWEEN_EMAILS_MAX=180

# Scraping Configuration
MAX_THERAPISTS_PER_STATE=100
DELAY_BETWEEN_REQUESTS_MIN=2
DELAY_BETWEEN_REQUESTS_MAX=5
